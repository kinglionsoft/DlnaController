{
  "extends": "tslint-ionic-rules",
  "rules": {
    "no-duplicate-variable": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-unused-variable": [
      true
    ],
    "ordered-imports": {
      "options": {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    }
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
